navigator.userAgent.substring(8,9) >= 3;				
msg = new Array(20)		
msg[0] = "Roger's Computer Technology"						          
if (supported) {
			var regn = new Image()
			regn.src = "images/outsd3.gif"
			var rega = new Image()
 			rega.src = "images/dranim.gif"
			}
          function act(imgName,stat) {
			window.status = msg[stat]
			if (supported){
             		document[imgName].src = eval(imgName + 'a.src')
			}
          }
          function inact(imgName) {
			window.status = ""
          		if (supported){
				document[imgName].src = eval(imgName + 'n.src')
			}
          }
</script>
